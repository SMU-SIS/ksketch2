<?xml version="1.0" encoding="utf-8"?>
<s:Group xmlns:fx="http://ns.adobe.com/mxml/2009" 
		 xmlns:s="library://ns.adobe.com/flex/spark" width="100%" height="100%" xmlns:timeBar="views.canvas.components.timeBar.*">
	<fx:Script>
		<![CDATA[
			import views.canvas.KSketch_CanvasView;
		]]>
	</fx:Script>
	<fx:Declarations>
		<timeBar:KTouchTimeControlFloatingLabel id="floatingLabel"/>
	</fx:Declarations>
	<s:Rect width="100%" height="100%">
		<s:fill>
			<s:SolidColor color="0xE7E7E7"/>
		</s:fill>
	</s:Rect>
	<s:Group width="100%" height="100%">
		<s:layout>
			<s:VerticalLayout paddingLeft="3" paddingRight="3" paddingTop="10" paddingBottom="10"
							  verticalAlign="middle" horizontalAlign="center"/>
		</s:layout>
		<s:Group width="92%" height="20" id="contentGroup">
			<s:Rect width="100%" height="100%" id="backgroundFill">
				<s:fill>
					<s:SolidColor color="0xFFFFFF"/>
				</s:fill>
				
			</s:Rect>
			<s:Rect width="100%" height="100%" id="timeFill">
				<s:fill>
					<s:SolidColor color="{KSketch_CanvasView.CANVAS_BUTTON_COLOR}" alpha="{KSketch_CanvasView.CANVAS_BUTTON_BLEND}"/>
				</s:fill>
			</s:Rect>
			<s:Group id="markerDisplay"/>
		</s:Group>
	</s:Group>
</s:Group>
