<?xml version="1.0" encoding="utf-8"?>
<s:Group xmlns:fx="http://ns.adobe.com/mxml/2009" 
		 xmlns:s="library://ns.adobe.com/flex/spark" width="100%" height="40">
	
	<s:Rect width="100%" height="100%" alpha="0">
		<s:fill>
			<s:SolidColor color="0x989898"/>
		</s:fill>
	</s:Rect>
	
	<s:Group width="100%" height="100%">
		<s:layout>
			<s:VerticalLayout paddingLeft="3" paddingRight="3" paddingTop="10" paddingBottom="10"/>
		</s:layout>
		<s:Group width="100%" height="100%">
			<s:Rect width="100%" height="100%" id="backgroundFill">
				<s:fill>
					<s:SolidColor color="0xF2F2F2"/>
				</s:fill>
				<s:stroke>
					<s:SolidColorStroke color="0xE6E6E6" weight="1.5"/>
				</s:stroke>
			</s:Rect>
			<s:Rect width="100%" height="100%" id="timeFill">
				<s:fill>
					<s:SolidColor color="0x585858"/>
				</s:fill>
			</s:Rect>
		</s:Group>
	</s:Group>
</s:Group>
